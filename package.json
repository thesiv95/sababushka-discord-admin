{
  "name": "discordbot-admin-backend",
  "version": "1.5.1",
  "description": "CRUD commands for Discord Sababot and for Sababot's admin panel",
  "main": "index.ts",
  "scripts": {
    "prestart": "echo Compiling... && npm run build",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "dev": "cross-env NODE_ENV=dev nodemon index.ts",
    "build": "tsc -p .",
    "changelog": "standard-version",
    "pushProd": "git push heroku main"
  },
  "keywords": [
    "discord",
    "express",
    "typescript"
  ],
  "author": "Ilya Soloveychik <dj.active.17@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mongoose": "^6.2.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "winston": "^3.6.0",
    "@types/express": "^4.17.13",
    "@types/cors": "^2.8.12"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.0.1",
    "nodemon": "^2.0.15",
    "standard-version": "^9.3.2"
  },
  "engines": {
    "node": "16.x"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
